<?xml version="1.0"?>
<ivysettings>
  <settings defaultResolver="chain"/>
  <property name="sosy.repo.url" value="http://www.sosy-lab.org/ivy"/>
  <resolvers>
    <chain name="chain">
      <url name="Sosy-Lab" descriptor="required">
        <ivy pattern="${sosy.repo.url}/[organisation]/[module]/ivy-[revision].xml"/>
        <artifact pattern="${sosy.repo.url}/[organisation]/[module]/[artifact]-[revision](-[classifier]).[ext]"/>
      </url>
      <ibiblio name="central" m2compatible="true"/>
    </chain>
  </resolvers>
</ivysettings>
